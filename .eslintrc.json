{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-extra-parens": "off",
    "no-console": "off"
  }
}
